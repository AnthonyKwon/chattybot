{
  "name": "chattybot",
  "version": "3.2.0",
  "description": "An Discord Text-To-Speech Bot",
  "main": "main.js",
  "scripts": {
    "start": "node .",
    "dev": "npm run development",
    "development": "NODE_ENV=development nodemon --inspect .",
    "register": "SLASH_ACTION=register node .",
    "registerDev": "NODE_ENV=development SLASH_ACTION=register node .",
    "registerAndRun": "npm run register && npm run start",
    "registerAndRunDev": "npm run registerDev && npm run development",
    "unregister": "SLASH_ACTION=unregister node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnthonyKwon/chattybot.git"
  },
  "author": "Anthony Kwon",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.16.1",
    "@google-cloud/text-to-speech": "^5.2.0",
    "discord.js": "^14.14.1",
    "json5": "^2.2.3",
    "sodium": "^3.0.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}
