{
  "name": "chattybot",
  "version": "3.0.1",
  "description": "An Discord Text-To-Speech Bot",
  "main": "main.js",
  "scripts": {
    "start": "node .",
    "dev": "npm run development",
    "development": "NODE_ENV=development nodemon --inspect .",
    "register": "NODE_ENV=development SLASH_ACTION=register node .",
    "devRegister": "NODE_ENV=development SLASH_ACTION=devRegister node .",
    "unregister": "NODE_ENV=development SLASH_ACTION=unregister node ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnthonyKwon/chattybot.git"
  },
  "author": "Anthony Kwon",
  "license": "GPL-2.0-or-later",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "@google-cloud/text-to-speech": "^4.2.3",
    "discord.js": "^14.14.1",
    "sodium": "^3.0.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
}